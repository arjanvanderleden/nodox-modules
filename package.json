{
  "name": "nodox-modules",
  "version": "0.0.3",
  "description": "Modules to be used with Nodox",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:test": "tsc -p ./test/tsconfig.json",
    "pretest": "npm run build && npm run build:test",
    "test": "mocha --reporter spec",
    "clean:dist": "del-cli ./dist",
    "prebuild": "npm run clean:dist",
    "build": "tsc",
    "precover": "npm run pretest",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arjanvanderleden/nodox-modules.git"
  },
  "keywords": [
    "Nodox"
  ],
  "author": "arjan.vander.leden@stepup.nl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arjanvanderleden/nodox-modules/issues"
  },
  "homepage": "https://github.com/arjanvanderleden/nodox-modules#readme",
  "dependencies": {
    "svg.js": "^2.6.3",
    "nodox-core": "^0.1.3",
    "color-convert": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.20",
    "@types/chai": "^4.0.3",
    "@types/color-convert": "^1.9.0",
    "@types/mocha": "^2.2.41",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "del-cli": "^1.1.0",
    "iconv-lite": "^0.4.18",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "typescript": "^2.4.2"
  }
}
